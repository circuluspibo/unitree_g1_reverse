<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="14.5dip" />
            <solid android:color="@color/common_white" />
            <corners android:radius="12.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <size android:height="10.0dip" />
                <gradient android:startColor="#ff6879e4" android:endColor="#ff495abf" android:angle="90.0" android:type="linear" />
                <corners android:radius="12.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>